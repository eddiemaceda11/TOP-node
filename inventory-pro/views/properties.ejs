<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% if (locals.categorizedProperties) {%> <% categorizedProperties.forEach(property => { %>
      <li>ID: <%= property.id %>, Location: <%= property.city %> <%= property.zipcode %></li>
      <a href="/property/<%= property.id %>">View Property</a>
      <% }); %> <% } %>
    </ul>
  </body>
</html>
